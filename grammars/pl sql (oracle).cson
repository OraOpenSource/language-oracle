      "4":
        name: "entity.name.function.oracle"
    match: "(?i)^\\s*(OVERRIDING)?\\s*(MEMBER|CONSTRUCTOR|STATIC)?\\s*(function|procedure)\\s+([-a-z0-9_]+)"
    name: "meta.procedure.oracle"
  }
  {
    match: "[!<>:]?=|<>|<|>|\\+|(?<!\\.)\\*|-|(?<!^)/|\\|\\|"
    name: "keyword.operator.oracle"
  }
  {
    match: "(?i)\\b(true|false|null)\\b"
